<h3>My Reviews</h3>

<% @reviews.each do |review| %>
  <h4>name: <%= review.location.nickname %></h4>
  latitude: <%= review.location.lat %><br />
  longitude: <%= review.location.lon %><br />
  stability: <%= review.stability %><br />
  content: <%= review.content %><br />

  <p>
  <%= link_to 'Show', review %>
  <%= link_to 'Edit', edit_review_path(review)%>
  <%= link_to 'Destroy', review, method: :delete, data: { confirm: 'Are you sure you want to delete this?' } %>
  </p>

<% end %>
