<div>

  <h2>Nearby anchorages</h2>
  <h4>(within 10km of your current location at <%= @current_lat.round(6) %>, <%= @current_lon.round(6) %> )</h4>
  <% @locations.each do |location| %>
    <h4><%= link_to location.nickname, location_path(location) %></h4>
    <p>Latitude: <%= location.lonlat.lat %></p>
    <p>Longitude: <%= location.lonlat.lon %></p>
  <% end %>

</div>
