<%= f.label "Date Visited:" %>
<%= f.datetime_field :date_visited, value: Time.now.strftime("%Y-%m-%dT%H:%M") %><br>

<%= f.label "Stability (10 is maximum stability):" %>
<%= f.range_field :stability, in: 0..10, step: '1' %><br>

<%= f.label "Content:" %>
<%= f.text_area :content %><br>

<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
<%= hidden_field_tag :location_id, value: nil %>
